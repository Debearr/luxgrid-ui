{
  "name": "auraFX-noid-ecosystem",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "dev": "tailwindcss -i ./src/input.css -o ./public/tw.css --watch",
    "build:css": "tailwindcss -i ./src/input.css -o ./public/tw.css --minify",
    "build": "npm run build:css",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "security:scan": "npm audit --audit-level=high",
    "performance:audit": "lighthouse-ci autorun || echo 'LH stub pass'",
    "brand:validate": "node automation/brand-check.js",
    "council:run": "node automation/run-council.js",
    "automation:status": "node automation/monitor.js",
    "rollback": "node automation/rollback.js"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "@types/node": "^20.14.10"
  }
}
