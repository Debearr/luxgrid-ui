{
  "version": 2,
  "framework": null,
  "buildCommand": "",
  "installCommand": "",
  "outputDirectory": "public",
  "routes": [
    { "src": "/health", "dest": "/api/health.js" },
    { "src": "/(.*)", "dest": "/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Council-Validated", "value": "true" },
        { "key": "X-Brand-Compliant", "value": "luxury-street" }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
