{
  "version": 2,
  "framework": "nextjs",
  "env": {
    "AI_COUNCIL_ENABLED": "true",
    "ZERO_TOUCH_DEPLOY": "true",
    "BRAND_VALIDATION": "true"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Council-Validated", "value": "true" },
        { "key": "X-Brand-Compliant", "value": "luxury-street" }
      ]
    }
  ]
}
