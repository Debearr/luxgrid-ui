{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Council-Validated", "value": "true" },
        { "key": "X-Brand-Compliant", "value": "luxury-street" }
      ]
    }
  ],
  "outputDirectory": "public"
}
